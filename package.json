{
  "name": "vite-plugin-webpackchunkname",
  "version": "1.0.0",
  "description": "A webpackChunkName plugin for Vite.",
  "main": "dist/vite-plugin-webpackchunkname.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "jest --no-cache --detectOpenHandles",
    "lint": "eslint src --ext .js,.ts",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainLiao/vite-plugin-webpackchunkname.git"
  },
  "keywords": [
    "vite plugin",
    "vite webpackchunkname",
    "rollup webpackChunkName",
    "manualChunks"
  ],
  "type": "module",
  "author": "CaptainLiao",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-alias": "*",
    "@rollup/pluginutils": "*",
    "es-module-lexer": "*",
    "magic-string": "*"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "rollup": "*",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vite": "*",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node script/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  }
}
